#!/usr/bin/perl
#
# Remove strings with variable contents (date, time, version number,
# stats) from test case output
#

while(<>) {
    next if( /^ *Date:/ );
    next if( /^now=/ );
    next if( /^\^AVia / );
    next if( /msgs\/s/ );
    next if( /Kbyte\/s/ );
    next if( /^Free disk space/ );
    next if( /unique.*lock/ );
    next if( /^From [^ ]/ );
    next if( /^\s*id / );

    s/\.(mo|tu|we|th|fr|sa|su)\d/.xx0/g;
    s/FIDOGATE \d+\.\d+\.\d+(-\S+\d+)*/FIDOGATE x.x.x/g;
    s/Rev *: *\d+\.\d+/Rev : x.x/;
    s/\d+$/123456789/ if( /^history:/ );
    s/(\S+ .+ -t) \d{10} (.)/$1 0 $2/g;
    s/(Sun|Mon|Tue|Wed|Thu|Fri|Sat) (Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)\s+\d+ \d{2}:\d{2}:\d{2} \d{4}/Fri Any 13 00:00:00 2002/g;

    print;
}
